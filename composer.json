{
    "name": "brew/intelipost",
    "description": "Laravel package for Intelipost shipping quote integration",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Douglas Maia",
            "email": "douglas@brew.com.vc"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0",
        "illuminate/database": "^11.0",
        "illuminate/http": "^11.0",
        "guzzlehttp/guzzle": "^7.8"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0.3",
        "pestphp/pest": "^3.5",
        "pestphp/pest-plugin-laravel": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Brew\\Intelipost\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Brew\\Intelipost\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Brew\\Intelipost\\Providers\\IntelipostServiceProvider"
            ],
            "aliases": {
                "Intelipost": "Brew\\Intelipost\\Facades\\Intelipost"
            }
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
